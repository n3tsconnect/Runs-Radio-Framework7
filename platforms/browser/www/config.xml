<?xml version='1.0' encoding='utf-8'?>
<widget id="com.runsradio.connect" version="4.5.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Runs Radio</name>
    <description>
        Run's Radio App
    </description>
    <author email="stormrider136@gmail.com" href="http://www.rafipriatna.web.id">
        Rafi Priatna Kasbiantoro
    </author>
    <content src="index.html" />
    <access origin="*" />
    <icon src="logo.png" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
</widget>
